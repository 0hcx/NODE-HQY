<div class="right-wrapper">
    <section class="content-header">
        <h1>学生管理</h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> 学生管理</a></li>
            <li class="active">添加学生</li>
        </ol>
    </section>
    <section class="content mobile-width">
        <div class="box">
            <form id="defaultForm" action="">
                <div class="box-body pad">
                    <div class="form-group">
                        <label>学生学号</label>
                        <input type="number" class="form-control" id="usr" required>
                    </div>
                    <div class="form-group">
                        <label>学生姓名</label>
                        <input type="text" class="form-control" id="student-name" required>
                    </div>
                    <div class="form-group">
                        <label>初始密码</label>
                        <input type="password" class="form-control" id="pwd" required>
                    </div>
                    <div class="form-group">
                        <label>输入重复密码</label>
                        <input type="password" class="form-control" id="pwd-repeat" required>
                    </div>
                </div>
                <div class="box-footer user-edit">
                    <button type="submit" class="btn btn-primary pull-right" id="registerBtn">确认添加</button>
                </div>
            </form>
        </div>
    </section>
</div>
<script src="/onlineExamSystem/addStuAdmin.js"></script>
