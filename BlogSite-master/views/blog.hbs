<div class="content-wrapper">
	<section class="content">
			<div class="blog-list">
				<figure>
					<img src="/img/login.png">
				</figure>
				<ul>
					<h3>{{title}}</h3>
					<p>{{content}}</p>
				<span class="postdate">
					<b>{{timeFromNow meta.createAt}}({{formatDate meta.createAt}})</b>
				</span>
					<div class="menu-block"><a href="/pdf/{{_id}}" class="btn btn-primary btn-sm">导出pdf</a></div>
				</ul>

			</div>
	</section>
</div>